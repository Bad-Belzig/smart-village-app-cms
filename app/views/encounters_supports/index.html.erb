<div class="text-center">
  <h1 class="h3 mb-0 font-weight-bold">
    Nutzer und Begegnungen einsehen
  </h1>
</div>

<div class="row justify-content-center">
  <div class="col-lg-5">
    <%= form_with(url: encounters_supports_validate_path, local: true) do |f| %>
       <div class="row">
        <div class="col">
          <div class="form-group">
            <label for="description">Support-ID *</label>
            <%= f.text_field :support_id, required: true, class: "form-control" %>
          </div>
        </div>
      </div>

      <div class="row justify-content-center mt-3 pb-4">
        <%= f.submit "Daten jetzt anzeigen", class: "btn btn-primary" %>
      </div>
    <% end %>
  </div>
</div>
