<%= form.fields_for :organizer, record.organizer do |fo|  %>
  <div class="row">
    <div class="col">
      <div class="form-group">
        <label for="description">Name des Organisators</label>
        <%= fo.text_field :name, class: "form-control" %>
      </div>
    </div>
  </div>

  <%= fo.fields_for :contact, record.organizer.contact do |fc|  %>
    <%= render partial: "shared/partials/contact_form", locals: { form: fc, contact: record.organizer.contact } %>
  <% end %>

  <%= fo.fields_for :address, record.organizer.address do |fa|  %>
    <%= render partial: "shared/partials/address_form", locals: { form: fa, address: record.organizer.address } %>
  <% end %>
<% end %>
